{
    "name": "sanctumlab-api",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "sourceRoot": "apps/sanctumlab-api/src",
    "projectType": "library",
    "tags": [],
    "targets": {
        "build": {
            "executor": "@nx/vite:build",
            "outputs": ["{options.outputPath}"],
            "options": {
                "outputPath": "dist/apps/sanctumlab-api",
                "main": "apps/sanctumlab-api/src/index.ts",
                "tsConfig": "apps/sanctumlab-api/tsconfig.lib.json",
                "assets": ["apps/sanctumlab-api/*.md"]
            }
        },
        "test": {
            "executor": "@nx/jest:jest",
            "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
            "options": {
                "jestConfig": "apps/sanctumlab-api/jest.config.ts"
            }
        }
    }
}
